<xml>
  <process
		name="NodeJs Example"
		script="main.js"
		arguments="${script}"
		filename="node.exe"
		workingDirectory="../scripts/nodejs/example/"
		language="Javascript"
		icon="node.png"
    autorun="true"
    backgroundColor="131, 205, 41"
		>
    <environment>
      <var name="PHP_INCLUDE_PATH" value="..\lib\php" override="true"/>
      <var name="PYTHON_INCLUDE_PATH" value="..\lib\python" override="true"/>
      <var name="NODE_INCLUDE_PATH" value="..\lib\nodejs" override="true"/>
    </environment>
    <listen>
      <signal name="KILL_PROC" action="kill"/>
      <signal name="RECYCLE_PROC" action="recycle"/>
      <signal name="RESET" action="reset"/>
    </listen>
  </process>
</xml>